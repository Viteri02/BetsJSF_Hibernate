<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://Java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<managed-bean>
		<managed-bean-name>bean</managed-bean-name>
		<managed-bean-class>modelo.bean.CrearPreguntaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>verbean</managed-bean-name>
		<managed-bean-class>modelo.bean.VerPreguntaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>Launch.xhtml</display-name>
		<from-view-id>/Launch.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>crear</from-outcome>
			<to-view-id>/CrearEvento.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Launch.xhtml</display-name>
		<from-view-id>/Launch.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>ver</from-outcome>
			<to-view-id>/VerEvento.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>CrearEvento.xhtml</display-name>
		<from-view-id>/CrearEvento.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>cerrar</from-outcome>
			<to-view-id>/Launch.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>VerEvento.xhtml</display-name>
		<from-view-id>/VerEvento.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>cerrar</from-outcome>
			<to-view-id>/Launch.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
